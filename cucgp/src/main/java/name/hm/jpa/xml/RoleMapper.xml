<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="name.hm.jpa.RoleMapper">
	<resultMap id="role" type="Role">
		<id property="roleId" column="role_id" javaType="Integer" />
		<result property="roleName" column="role_name" javaType="String" />
		<result property="valid" column="valid" javaType="name.hm.m.Role$Valid" />
	</resultMap>

	<select id="selectAll" resultType="Role">
		SELECT * FROM cucgp.`role` ORDER BY role_id ASC;
	</select>

</mapper>
