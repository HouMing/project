package org.wave.mvc.s;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.wave.jpa.Band;
import org.wave.mvc.m.dao.BandDAO;

@Service(value = "debugService")
public class DebugService
{
  @Autowired
  BandDAO bdao;
	
	public Band findBand()
	{
		return bdao.findBand();
	}
	
	public void insertBand(Band obj) {
		
	}
	
	public Band findBand(String id) {
		return bdao.findBand(id);
	}
}
