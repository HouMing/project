package org.wave.define;

import java.util.List;


public class Inorganic implements IMole, I
{
	public String _id;
  public String name;
  public List<String> molecules;
  
  protected Integer mole;

  public Inorganic(String _id, String name) {
  	this._id = _id;
  	this.name = name;
  }
  
	public Integer getMole()
	{
		return mole;
	}

	public void setMole()
	{
		mole += 1;
	}

	public String get_id()
	{
		return _id;
	}
	
	public Organic organic(Wave band, List<I> args) {
		if (!band.molecules.contains(this._id)) {
			return null;
		}
		new Organic();
		return null;
	}
	
}
