package org.wave.jpa.dao.exception;

public class DAOCreateException extends Exception
{
	private static final String NULL_OBJECT = "nullObject";

	public DAOCreateException(DAOCreateException.Type type)
	{
	}

	protected DAOCreateException(String msg)
	{
		super(msg);
	}

	public enum Type
	{
		NullObject("nulObject");
		protected String value;

		Type(String value)
		{
			this.value = value;
		}
	}
}
