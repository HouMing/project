package org.wave.jpa;

import java.util.List;

import org.wave.define.Mole;

public class Inorganic implements Mole
{
	public String _id;
  public String name;
  public List<Molecule> molecules;

  protected Integer mole;

	public Integer getMole()
	{
		return mole;
	}

	public void setMole()
	{
		mole += 1;
	}
}
